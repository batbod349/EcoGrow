<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}EcoGrow
			{% endblock %}
		</title>
		<link rel="icon" src="../images/logo.png">
		<meta
		name="viewport" content="width=device-width, initial-scale=1.0">

		{# Polices #}
		<link href="https://fonts.cdnfonts.com/css/rawles" rel="stylesheet">
<head>
    <!-- Ajout de Swiper CSS -->
    <link rel="stylesheet" href="https://unpkg.com/swiper/swiper-bundle.min.css" />
</head>
<body>

    <!-- Ajout de Swiper JS -->
    <script src="https://unpkg.com/swiper/swiper-bundle.min.js"></script>
</body>

		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
			{% block importmap %}
				{{ importmap('app') }}
			{% endblock %}
		{% endblock %}
	</head>
	<header>
		{% block header %}
		{% endblock %}
	</header>
	<body class="bg-white pb-32 h-full"> {% block body %}{% endblock %}
		</body>
		<footer>
			{% block footer %}
				{% include 'components/navigation/bottomNavBar.html.twig' %}
			{% endblock %}
		</footer>
	</html>
